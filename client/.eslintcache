bel>\n                            <input type=\"email\" name=\"email\" value={this.state.email} onChange={event => this.handleChange(event)}/>\n\n                            <label>Mot de passe</label>\n                            <input type=\"password\" name=\"password\" value={this.state.password} onChange={event => this.handleChange(event)}/>\n                        \n                            <button className=\"login-button\">Connexion</button>\n                        </form>\n                    <p>\n                    <Link to={\"/signup\"}> Créez un compte gratuitement</Link>\n                    </p>\n                    </div>\n                    <div className=\"container-images\">\n                        <div className=\"images-container\">\n                            <div className=\"image-one\"></div>\n                            <div className=\"image-two\"></div>\n                        </div>\n                        <div className=\"container-titre\">\n                            <h2>\"L'essence de l'indépendance est de pouvoir faire quelque chose pour soi-même.\"</h2>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default Login;","/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Footer/Footer.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Homepage/Homepage.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Auth/Signup.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Profile/Profile.js",["61"],"import React from 'react';\nimport service from '../Auth/auth-service';\nimport { Link, Redirect } from 'react-router-dom';\nimport './Profile.css';\n\nclass Profile extends React.Component {\n    state = {\n        user : {}\n    }\n\n    getUserProfile = () => {\n        const params = this.props.match.params;\n        service.get(`/auth/profile/${params.id}`)\n        .then(responseFromApi => {\n            const theUser = responseFromApi.data;\n            this.setState({user: theUser});\n        })\n        .catch((err) => {\n            console.log(`Le user profile n'est pas le bon`, err);\n        })\n    }\n\n    //Function componentDiMount()\n\n    componentDiMount(){\n        this.getUserProfile()\n    }\n\n    render(){\n        return(\n            <div id=\"profile\">\n                <div className=\"section-profile\">\n                    <div className=\"profile-presentation\">\n                        <h1>Bienvenue {this.state.user.username}</h1>\n                        <h2>Ajoutez les premières fois de votre enfant</h2>\n                        <Link to={`/etiquette`}><button className=\"profile-add\">Ajouter une nouvelle étiquette</button></Link>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n\n}\n\nexport default Profile;","/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Etiquette/ListEtiquette.js",["62"],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Etiquette/AddEtiquette.js",["63"],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"64","replacedBy":"68"},{"ruleId":"66","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":5,"column":9,"nodeType":"72","messageId":"73","endLine":5,"endColumn":14},{"ruleId":"70","severity":1,"message":"74","line":39,"column":16,"nodeType":"72","messageId":"73","endLine":39,"endColumn":24},{"ruleId":"70","severity":1,"message":"75","line":3,"column":16,"nodeType":"72","messageId":"73","endLine":3,"endColumn":24},{"ruleId":"76","severity":2,"message":"77","line":41,"column":34,"nodeType":"78","endLine":41,"endColumn":38},{"ruleId":null,"fatal":true,"severity":2,"message":"79","line":20,"column":9},"no-native-reassign",["