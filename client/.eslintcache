        <button onClick = {(e) => {\n                                    logout()\n                                    .then(() => props.updateUser(null))\n                                }}>Logout</button>\n                                </Link>\n                            </div>\n                        </div>\n                    ) : (\n                        <button className=\"button-login\"><Link to=\"/login\">Connexion</Link></button>  \n                    )}\n                    </ul>\n                </div>\n            </div>\n        )   \n}\n\nexport default Navbar;","/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Auth/auth-service.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Auth/Login.js",["66"],"import React from 'react';\nimport './Auth.css';\nimport {login} from './auth-service';\nimport { Link, Redirect } from 'react-router-dom';\n\nclass Login extends React.Component {\n    state = {\n        email:\"\",\n        password:\"\",\n        redirect: false\n    }\n\n    //function handleFormSubmit\n    handleFormSubmit = (event) => {\n        event.preventDefault();\n        const email = this.state.email;\n        const password = this.state.password;\n\n        login(email,password)\n        .then(response => {\n            this.setState({\n               email:\"\",\n               password:\"\",\n               redirect: true\n            })\n            this.props.updateUser(response);\n        })\n        .catch(err => console.log(err));\n    }\n\n    //function handleChange\n    handleChange = (event) => {\n        const {name, value} = event.target;\n        this.setState({[name] : value});\n    }\n    \n    //Les fonctions handleFormSubmit et handleChange dans notre formulaire\n    render() {\n        const {redirect} = this.state;\n        if(this.props.user?._id){\n            return <Redirect to={`/profile/${this.props.user._id}`}/>\n        }\n        return(\n            <div id=\"login\">\n                <div className=\"section-login\">\n                    <div className=\"form-login\">\n                        <h1>Connectez-vous à vore compte</h1>\n                        <form onSubmit={this.handleFormSubmit} className=\"form-login\">\n                            <label>Email</label>\n                            <input type=\"email\" name=\"email\" value={this.state.email} onChange={event => this.handleChange(event)}/>\n\n                            <label>Mot de passe</label>\n                            <input type=\"password\" name=\"password\" value={this.state.password} onChange={event => this.handleChange(event)}/>\n                        \n                            <button className=\"login-button\">Connexion</button>\n                        </form>\n                    <p>\n                    <Link to={\"/signup\"}> Créez un compte gratuitement</Link>\n                    </p>\n                    </div>\n                    <div className=\"container-images\">\n                        <div className=\"images-container\">\n                            <div className=\"image-one\"></div>\n                            <div className=\"image-two\"></div>\n                        </div>\n                        <div className=\"container-titre\">\n                            <h2>\"L'essence de l'indépendance est de pouvoir faire quelque chose pour soi-même.\"</h2>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default Login;","/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Footer/Footer.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Homepage/Homepage.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Auth/Signup.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Profile/Profile.js",["67"],"import React from 'react';\nimport service from '../Auth/auth-service';\nimport { Link, Redirect } from 'react-router-dom';\nimport './Profile.css';\nimport ListEtiquette from '../Etiquette/ListEtiquette';\n\n\nclass Profile extends React.Component {\n    state = {\n        user : {}\n    }\n\n    getUserProfile = () => {\n        const params = this.props.match.params;\n        service.get(`/auth/profile/${params.id}`)\n        .then(responseFromApi => {\n            const theUser = responseFromApi.data;\n            this.setState({user: theUser});\n        })\n        .catch((err) => {\n            console.log(`Le user profile n'est pas le bon`, err);\n        })\n    }\n\n    //Function componentDiMount()\n\n    componentDiMount(){\n        this.getUserProfile()\n    }\n\n    render(){\n        return(\n            <div id=\"profile\">\n                <div className=\"section-profile\">\n                    <div className=\"profile-presentation\">\n                        <h1>Bienvenue {this.props.user.username}</h1>\n                        <h2>Ajoutez les premières fois de votre enfant</h2>\n                        <Link to={`/etiquette/ajout`}><button className=\"profile-add\">Ajouter une nouvelle étiquette</button></Link>\n                            <div className=\"show-etiquette\">\n                                <ListEtiquette/>\n                            </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n\n}\n\nexport default Profile;","/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Etiquette/ListEtiquette.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Etiquette/AddEtiquette.js",["68","69","70"],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Etiquette/DetailsEtiquette.js",["71"],"import React from 'react';\nimport {Link} from 'react-router-dom';\nimport service from '../Auth/auth-service';\nimport './Etiquette.css';\n\nclass DetailsEtiquette extends React.Component{\n    state = {\n        etiquette : {}\n    }\n\n    //Function get0neEtiquette\n\n    getOneEtiquette = () => {\n        const {params} = this.props.match\n        service.get(`/etiquette/${params.id}`)\n        .then(responseFromApi => {\n            const theEtiquette = responseFromApi.data;\n            this.setState({etiquette: theEtiquette})\n        })\n        .catch((err) => {\n            console.log('erreur pour charger', err)\n        })\n    }\n\n    //Function componentDiMount()\n\n    componentDiMount(){\n        this.getOneEtiquette()\n    }\n\n    //Function deleteEtiquette()\n    deleteEtiquette = () => {\n        const {params} = this.props.match\n        service.delete(`/etiquette/${params.id}`)\n        .then(() => {\n            this.props.history.push(`/profile/${params.id}`); //Quand ça deletera, le user sera redirigé sur le profil \n        })\n        .catch((err) => {\n            console.log(`il y a une erreur dans la suppression de l'etiquette`, err);\n        })\n    }\n\n\n\n    render(){\n        return(\n            <div id=\"section-details-etiquette\">\n                <div className=\"container-details-etiquette\">\n                    <div className=\"details-etiquette\">\n                        <div className=\"image\">\n                            <img src={this.state.etiquette.imageUrl} alt=\"image de l'étiquette\"/>\n                        </div>\n                        <div className=\"details\">\n                            <h1>{this.state.etiquette.title}</h1>\n                            <p><span>Lieu: {this.state.etiquette.lieu}</span></p>\n                            <p><span>Date: {this.state.etiquette.date}</span></p>\n                            <p><span>Commentaire: {this.state.etiquette.commentaire}</span></p>\n                        </div>\n                        <div className=\"differents-buttons\">\n                            <div className=\"button-modified\">\n                                <Link to={'/modify-etiquette'}>Modifier</Link>\n                            </div>\n                            <div className=\"button-delete\">\n                                <p className=\"deleteButton\" onClick={() => this.deleteEtiquette()}>Supprimer</p>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default DetailsEtiquette;",{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":10,"column":8,"nodeType":"78","messageId":"79","endLine":10,"endColumn":21},{"ruleId":"72","replacedBy":"80"},{"ruleId":"74","replacedBy":"81"},{"ruleId":"76","severity":1,"message":"82","line":5,"column":9,"nodeType":"78","messageId":"79","endLine":5,"endColumn":14},{"ruleId":"76","severity":1,"message":"83","line":39,"column":16,"nodeType":"78","messageId":"79","endLine":39,"endColumn":24},{"ruleId":"76","severity":1,"message":"84","line":3,"column":16,"nodeType":"78","messageId":"79","endLine":3,"endColumn":24},{"ruleId":"76","severity":1,"message":"85","line":3,"column":9,"nodeType":"78","messageId":"79","endLine":3,"endColumn":13},{"ruleId":"76","severity":1,"message":"82","line":4,"column":9,"nodeType":"78","messageId":"79","endLine":4,"endColumn":14},{"ruleId":"76","severity":1,"message":"83","line":52,"column":16,"nodeType":"78","messageId":"79","endLine":52,"endColumn":24},{"ruleId":"86","severity":1,"message":"87","line":51,"column":29,"nodeType":"88","endLine":51,"endColumn":98},"no-native-reassign",["89"],"no-negated-in-lhs",["90"],"no-unused-vars","'ListEtiquette' is defined but never used.","Identifier","unusedVar",["89"],["90"],"'login' is defined but never used.","'redirect' is assigned a value but never used.","'Redirect' is def