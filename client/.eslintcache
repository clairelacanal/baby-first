ue} = event.target;\n        this.setState({[name] : value});\n    }\n    \n    //Les fonctions handleFormSubmit et handleChange dans notre formulaire\n    render() {\n        if(this.state.redirect){\n            return <Redirect to=\"/profile\"/>\n        }\n        return(\n            <div id=\"login\">\n                <div className=\"section-login\">\n                    <div className=\"form-login\">\n                        <form onSubmit={this.handleFormSubmit} className=\"form-login\">\n                            <label>Email:</label>\n                            <input type=\"email\" name=\"email\" value={this.state.email} onChange={event => this.handleChange(event)}/>\n\n                            <label>Mot de passe:</label>\n                            <input type=\"password\" name=\"password\" value={this.state.password} onChange={event => this.handleChange(event)}/>\n                        \n                            <button className=\"button-signup\">Connexion</button>\n                        </form>\n                    <p>\n                        <Redirect to={\"/signup\"}>Cr√©ez un compte gratuitement</Redirect>\n                    </p>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default Login;","/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Footer/Footer.js",[],"/home/claire/personnel-projects/react-personal-projects/baby-first/client/src/components/Homepage/Homepage.js",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":3,"column":10,"nodeType":"53","messageId":"54","endLine":3,"endColumn":16},{"ruleId":"51","severity":1,"message":"55","line":3,"column":18,"nodeType":"53","messageId":"54","endLine":3,"endColumn":23},{"ruleId":"51","severity":1,"message":"56","line":7,"column":8,"nodeType":"53","messageId":"54","endLine":7,"endColumn":13},{"ruleId":"47","replacedBy":